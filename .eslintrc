{
  "rules": {
    "indent": [ 2, 2, { "VariableDeclarator": { "var": 2, "let": 2, "const": 3 } } ],
    "linebreak-style": [ 2, "unix" ],
    "no-unused-vars": [ 2, { "args": "after-used" } ],
    "quotes": [ 2, "single" ],
    "react/jsx-key": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-pascal-case": 1,
    "react/jsx-uses-vars": 1,
    "semi": [ 2, "always" ],
    "no-console": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react"
  ]
}